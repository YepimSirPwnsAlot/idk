KJ.Name = "KJ"
KJ.Parent = ScrollingFrame
KJ.BackgroundColor3 = Color3.fromRGB(250, 128, 114)
KJ.Position = UDim2.new(0.00934579596, 0, 0.306296647, 0)
KJ.Size = UDim2.new(0, 419, 0, 36)
KJ.Font = Enum.Font.SourceSansBold
