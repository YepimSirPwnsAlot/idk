	"I got the idea to make this from Mr.Flamgo:D",
	"Mommy tells me im special",
	"Old acc is SirPwnsALot, follow my old account NOW",
}

local function cycleInOrder()
	local index = 1
	while true do
		Welcome2.Text = messages[index]
		index = index + 1
		if index > #messages then
			index = 1
		end
		wait(30)
	end
end

task.spawn(cycleInOrder)

