    miniBtn.Position = UDim2.new(1, -70, 0.5, 0)
    miniBtn.AnchorPoint = Vector2.new(0, 0.5)
    miniBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    miniBtn.Text = "-"
    miniBtn.TextColor3 = Color3.new(1, 1, 1)
    miniBtn.Font = Enum.Font.GothamBold
    miniBtn.Parent = topBar
    Instance.new("UICorner", miniBtn).CornerRadius = UDim.new(0, 6)

    local scrollingFrame = Instance.new("ScrollingFrame")
    scrollingFrame.Size = UDim2.new(1, -20, 1, -60)
    scrollingFrame.Position = UDim2.new(0, 10, 0, 50)
    scrollingFrame.BackgroundTransparency = 1
    scrollingFrame.ScrollBarThickness = 6
    scrollingFrame.ScrollBarImageColor3 = Color3.fromRGB(0, 255, 255)
    scrollingFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
    scrollingFrame.Parent = mainFrame

    local layout = Instance.new("UIListLayout", scrollingFrame)
    layout.Padding = UDim.new(0, 10)
    layout.HorizontalAlignment = Enum.HorizontalAlignment.Center

    local dragging, dragInput, dragStart, startPos
    topBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = mainFrame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then dragging = false end
            end)
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
            local delta = input.Position - dragStart
            mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)

    local minimized = false
    miniBtn.MouseButton1Click:Connect(function()
        minimized = not minimized
        local targetSize = minimized and UDim2.new(0, 400, 0, 40) or UDim2.new(0, 400, 0, 400)
        TweenService:Create(mainFrame, TweenInfo.new(0.3), {Size = targetSize}):Play()
        miniBtn.Text = minimized and "+" or "-"
    end)

    closeBtn.MouseButton1Click:Connect(function() screenGui:Destroy() end)

    local function addPlayerEntry(player)
        local entry = Instance.new("Frame")
        entry.Name = player.Name
        entry.Size = UDim2.new(0.95, 0, 0, 70)
        entry.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        entry.Parent = scrollingFrame
        Instance.new("UICorner", entry).CornerRadius = UDim.new(0, 10)

        local pfp = Instance.new("ImageLabel")
        pfp.Size = UDim2.new(0, 50, 0, 50)
        pfp.Position = UDim2.new(0, 10, 0.5, 0)
        pfp.AnchorPoint = Vector2.new(0, 0.5)
        pfp.BackgroundTransparency = 1
        pfp.Image = Players:GetUserThumbnailAsync(player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)
        pfp.Parent = entry
        Instance.new("UICorner", pfp).CornerRadius = UDim.new(1, 0)

        local infoLabel = Instance.new("TextLabel")
        infoLabel.Size = UDim2.new(1, -70, 1, -10)
        infoLabel.Position = UDim2.new(0, 70, 0, 5)
        infoLabel.BackgroundTransparency = 1
        infoLabel.TextColor3 = Color3.new(1, 1, 1)
        infoLabel.Text = player.DisplayName .. "\nJoined: " .. os.date("%x", os.time() - (player.AccountAge * 86400))
        infoLabel.Font = Enum.Font.Gotham
        infoLabel.TextSize = 11
        infoLabel.TextXAlignment = Enum.TextXAlignment.Left
        infoLabel.Parent = entry

        task.spawn(function()
            local start = os.clock()
            while entry and entry.Parent do
                local diff = math.floor(os.clock() - start)
                infoLabel.Text = player.DisplayName .. "\nJoined: " .. os.date("%x", os.time() - (player.AccountAge * 86400)) .. "\nSession: " .. diff .. "s"
                task.wait(1)
            end
        end)
    end

    Players.PlayerAdded:Connect(addPlayerEntry)
    Players.PlayerRemoving:Connect(function(p) if scrollingFrame:FindFirstChild(p.Name) then scrollingFrame[p.Name]:Destroy() end end)
    for _, p in pairs(Players:GetPlayers()) do addPlayerEntry(p) end
end)

local TW = CreateButton("Btn1", "xRay Vision")
Instance.new("UICorner", TW).CornerRadius = UDim.new(0, 6)

TW.MouseButton1Down:Connect(function()
for _, part in pairs(workspace:GetDescendants()) do
    if part:IsA("Part") then
        part.Transparency = 0.6
