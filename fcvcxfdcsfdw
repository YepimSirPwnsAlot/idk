local rightArm =
	character:FindFirstChild("RightHand") or
	character:FindFirstChild("Right Arm")

local function criarBloco(size, color)
	local bloco = Instance.new("Part")
	bloco.Size = size
	bloco.Anchored = true
	bloco.CanCollide = false
	bloco.Material = Enum.Material.SmoothPlastic
	bloco.Color = color
	bloco.Parent = workspace
	return bloco
end

local blocoBraco = criarBloco(
	Vector3.new(1.1, 0.7, 1.1),
	Color3.fromRGB(255, 0, 0)
)

local cilindro = Instance.new("Part")
cilindro.Shape = Enum.PartType.Cylinder
cilindro.Size = Vector3.new(0.2, 0.6, 0.6)
cilindro.Anchored = true
cilindro.CanCollide = false
cilindro.Material = Enum.Material.SmoothPlastic
cilindro.Color = Color3.fromRGB(255, 255, 255)
cilindro.Parent = workspace

local surfaceGui = Instance.new("SurfaceGui")
surfaceGui.Face = Enum.NormalId.Right
surfaceGui.AlwaysOnTop = true
surfaceGui.LightInfluence = 0
surfaceGui.SizingMode = Enum.SurfaceGuiSizingMode.PixelsPerStud
surfaceGui.PixelsPerStud = 100
surfaceGui.Parent = cilindro

local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(1, 0, 1, 0)
textLabel.BackgroundTransparency = 1
textLabel.Text = ""
textLabel.TextScaled = true
textLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
textLabel.Font = Enum.Font.GothamBold
textLabel.Rotation = 45
textLabel.Parent = surfaceGui

RunService.RenderStepped:Connect(function()
	if rightArm then
		local baseCFrame = rightArm.CFrame
		blocoBraco.CFrame = baseCFrame

		cilindro.CFrame =
			baseCFrame
			* CFrame.new(
				(blocoBraco.Size.X / 2) - 0.07,
				-0.01,
				-0.02
			)
	end
end)

local plr = game:GetService("Players").LocalPlayer  
local char = plr.Character or plr.CharacterAdded:Wait()  
local hum = char:FindFirstChild("Humanoid") or char:WaitForChild("Humanoid")  
local anim = hum:FindFirstChildOfClass("Animator") or hum:WaitForChild("Animator")  
local pack = plr:FindFirstChild("Backpack") or plr:WaitForChild("Backpack")  

if workspace:FindFirstChild("Heil") then  
	workspace:FindFirstChild("Heil"):Destroy()  
end  

local function getmodel()  
	return hum.RigType == Enum.HumanoidRigType.R6 and "R6"  
end  

local animation = Instance.new("Animation")  
animation.Name = "Heil"  
animation.Parent = workspace  
animation.AnimationId = getmodel() == "R6" and "rbxassetid://186904307"  

local tool = Instance.new("Tool")  
tool.Name = "Salute!"  
tool.RequiresHandle = false  
tool.Parent = pack  

local doing = false  
local animtrack = nil  

tool.Equipped:Connect(function()  
	doing = true  
	while doing do  
		if not animtrack then  
			animtrack = anim:LoadAnimation(animation)  
		end  
		animtrack:Play()  
		animtrack:AdjustSpeed(0.1)  
		task.wait(0.3)  

		while doing and animtrack and animtrack.TimePosition < 0.6 do  
			task.wait(1)  
		end  

		if animtrack then  
			animtrack:Stop()  
			animtrack:Destroy()  
			animtrack = nil  
		end  
	end  
end)  

tool.Unequipped:Connect(function()  
	doing = false  
	if animtrack then  
		animtrack:Stop()  
		animtrack:Destroy()  
		animtrack = nil  
	end  
end)  

hum.Died:Connect(function()  
	doing = false  
	if animtrack then  
		animtrack:Stop()  
		animtrack:Destroy()  
		animtrack = nil  
	end  
end)

