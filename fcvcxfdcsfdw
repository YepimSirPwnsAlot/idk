local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 6)
corner.Parent = FEr6

local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(0, 255, 255)
stroke.Thickness = 2
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
stroke.Parent = FEr6

local FE = Instance.new("TextButton")
FE.Name = "fwefwfwfwfwfeww"
FE.Size = UDim2.new(0.95, 0, 0, 38)
FE.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
FE.TextColor3 = Color3.fromRGB(0, 255, 255)
FE.Text = "FE Crystal Dance"
FE.Font = Enum.Font.GothamBold
FE.TextSize = 14
FE.Parent = TrollingPage
FE.MouseButton1Click:Connect(function()
	local MakeMsg = Instance.new("Message")
	MakeMsg.Text = "You need r6 for this so make sure you have r6"
	MakeMsg.Parent = Workspace
	wait(3)
	MakeMsg:Destroy()
	
	loadstring(game:HttpGet("https://pastebin.com/raw/vT1URaRJ"))() 
end)

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 6)
corner.Parent = FE

local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(0, 255, 255)
stroke.Thickness = 2
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
stroke.Parent = FE
