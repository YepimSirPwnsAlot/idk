local CyanTheme = CreateButton("Btn1", "Cyan (Default)")
Instance.new("UICorner", CyanTheme).CornerRadius = UDim.new(0, 6)

CyanTheme.MouseButton1Click:Connect(function()
	local themeColor = Color3.fromRGB(0, 255, 255)
    
    local foundStroke = CyanTheme:FindFirstChild("UIStroke")
    if foundStroke then
        foundStroke.Color = themeColor
    end
    
    CyanTheme.TextColor3 = themeColor
end)

local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(0, 255, 255)
stroke.Thickness = 2
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
stroke.Parent = CyanTheme

