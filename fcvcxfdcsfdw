pagesContainer.BackgroundTransparency = 1
pagesContainer.ClipsDescendants = true
pagesContainer.Parent = mainFrame

local allPages = {} 
local allTabs = {}
local currentSelectedTab = nil

local function addTab(name, order, isLast)
    local container = Instance.new("Frame")
    container.Name = name .. "_TabContainer"
    container.Size = UDim2.new(1, 0, 0, 40)
    container.BackgroundTransparency = 1
    container.LayoutOrder = order
    container.Parent = sidebar

    local tab = Instance.new("TextButton")
    tab.Size = UDim2.new(1, 0, 1, 0)
    tab.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    tab.TextColor3 = Color3.fromRGB(0, 255, 255)
    tab.Text = "  " .. name 
    tab.TextXAlignment = Enum.TextXAlignment.Left
    tab.Font = Enum.Font.GothamMedium
    tab.TextSize = 13
    tab.BorderSizePixel = 0
    tab.ZIndex = 3
    tab.Parent = container
    
    if not isLast then
        local div = Instance.new("Frame")
        div.Name = "TabDivider"
        div.Size = UDim2.new(1, 0, 0, 1)
        div.Position = UDim2.new(0, 0, 1, -1)
        div.BackgroundColor3 = Color3.fromRGB(0, 255, 255)
        div.BorderSizePixel = 0
        div.ZIndex = 5
        div.Parent = container
    end

    local page = Instance.new("ScrollingFrame")
    page.Name = name .. "_Page"
    page.Size = UDim2.new(1, 0, 1, 0)
    page.BackgroundTransparency = 1
    page.Visible = false
    page.ScrollBarThickness = 2
    page.ScrollBarImageColor3 = Color3.fromRGB(0, 255, 255)
    page.Parent = pagesContainer
    
    allPages[name] = page
    allTabs[name] = tab

    tab.MouseButton1Down:Connect(function()
        for tabName, p in pairs(allPages) do
            p.Visible = false
            allTabs[tabName].TextColor3 = Color3.fromRGB(0, 255, 255)
        end
        page.Visible = true
        tab.TextColor3 = Color3.fromRGB(255, 255, 255)
        currentSelectedTab = name
    end)

    tab.MouseEnter:Connect(function()
        if currentSelectedTab ~= name then tab.TextColor3 = Color3.fromRGB(0, 255, 255) end
    end)
    
    tab.MouseLeave:Connect(function()
        if currentSelectedTab ~= name then tab.TextColor3 = Color3.fromRGB(0, 255, 255) end
    end)
    
    return page
end

local tabList = {"Information", "Updates", "Player", "Fe/Admin Gui's", "Scripts", "Chat", "Trolling", "DM's", "Settings", "Themes"}

for i, name in ipairs(tabList) do
    local isLast = (i == #tabList)
    local newPage = addTab(name, i, isLast)
    if i == 1 then 
        newPage.Visible = true 
        allTabs[name].TextColor3 = Color3.fromRGB(0, 255, 255)
        currentSelectedTab = name
    end
end

local dragging, dragStart, startPos
topBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
end)

local minimized = false
minBtn.MouseButton1Down:Connect(function()
    if not minimized then
        topDivider.Visible = false
        pfpImg.Visible = false
        profileLine.Visible = false
        profileFrame.Visible = false
        
        mainFrame:TweenSize(UDim2.new(0, 500, 0, 47), "Out", "Quad", 0.3, true)
        minBtn.Text = "+"
    else
        mainFrame:TweenSize(UDim2.new(0, 500, 0, 400), "Out", "Quad", 0.3, true)
        minBtn.Text = "-"
        
        task.delay(0.2, function()
            topDivider.Visible = true
            pfpImg.Visible = true
            profileLine.Visible = true
            profileFrame.Visible = true
        end)
    end
    
    minimized = not minimized
end)

closeBtn.MouseButton1Click:Connect(function()
    local TweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
    
    local shrinkTween = TweenService:Create(mainFrame, tweenInfo, {
        Size = UDim2.new(0, 0, 0, 0),
        BackgroundTransparency = 1
    })
    
    mainStroke.Enabled = false
    
    shrinkTween:Play()
    
    shrinkTween.Completed:Connect(function()
        screenGui:Destroy()
    end)
end)

