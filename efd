EcholiteHubTheCondoDestroyerSeperatorUIStroke.Parent = EcholiteHubTheCondoDestroyerSeperator


local success, result = pcall(function()
    local TweenService = game:GetService("TweenService")
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer

    local gui = Instance.new("ScreenGui")
    gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    gui.ResetOnSpawn = false
    gui.Parent = player:WaitForChild("PlayerGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 380, 0, 140)
    frame.Position = UDim2.new(0.5, -200, 0.5, -140)
    frame.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
    frame.BackgroundTransparency = 1
    frame.BorderSizePixel = 0
    frame.Parent = gui

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 15)
    corner.Parent = frame

    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(0, 255, 255)
    stroke.Thickness = 3
    stroke.Transparency = 1
    stroke.Parent = frame

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, -20, 0, 32)
    title.Position = UDim2.new(0, 10, 0, 8)
    title.BackgroundTransparency = 1
    title.Text = "Echolite Hub"
    title.TextColor3 = Color3.new(0, 255, 255)
    title.TextScaled = true
    title.Font = Enum.Font.GothamBold
    title.TextTransparency = 1
    title.Parent = frame

    local customImage = Instance.new("ImageLabel")
    customImage.Size = UDim2.new(0, 70, 0, 70)
    customImage.Position = UDim2.new(0.5, -35, 0, 44)
    customImage.BackgroundTransparency = 1
    customImage.Image = "rbxassetid://86733014304064"
    customImage.ImageTransparency = 1
    customImage.Parent = frame

    local subtitle = Instance.new("TextLabel")
    subtitle.Size = UDim2.new(1, -20, 0, 26)
    subtitle.Position = UDim2.new(0, 10, 0, 116)
    subtitle.BackgroundTransparency = 1
    subtitle.Text = "Loading this may take a bit..."
    subtitle.TextColor3 = Color3.fromRGB(180, 180, 180)
    subtitle.TextWrapped = false
	subtitle.TextSize = 10
	subtitle.Font = Enum.Font.Gotham
    subtitle.TextTransparency = 1
    subtitle.Parent = frame

    local loader = Instance.new("ImageLabel")
    loader.Size = UDim2.new(0, 28, 0, 28)
    loader.Position = UDim2.new(1, -40, 0, 112)
    loader.BackgroundTransparency = 1
    loader.Image = "rbxassetid://81120285184687"
    loader.ImageTransparency = 1
    loader.Parent = frame

    local fadeIn = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local fadeOut = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.In)

    TweenService:Create(frame, fadeIn, {
        Position = UDim2.new(0.5, -200, 0.5, -70),
        BackgroundTransparency = 0
    }):Play()

    TweenService:Create(title, fadeIn, { TextTransparency = 0 }):Play()
    TweenService:Create(subtitle, fadeIn, { TextTransparency = 0 }):Play()
    TweenService:Create(stroke, fadeIn, { Transparency = 0 }):Play()
    TweenService:Create(loader, fadeIn, { ImageTransparency = 0 }):Play()
    TweenService:Create(customImage, fadeIn, { ImageTransparency = 0 }):Play()

    local isRunning = true

    task.spawn(function()
        local dots = 0
        while isRunning do
            dots = (dots % 3) + 1
            subtitle.Text = "Loading" .. string.rep(".", dots)
            task.wait(0.35)
        end
    end)

    task.spawn(function()
        while isRunning do
            loader.Rotation = (loader.Rotation + 5) % 360
            task.wait(0.02)
        end
    end)

    task.spawn(function()
        while isRunning do
            TweenService:Create(customImage, TweenInfo.new(1.2, Enum.EasingStyle.Sine), {
                Position = UDim2.new(0.5, -35, 0, 42)
            }):Play()
            task.wait(1.2)

            TweenService:Create(customImage, TweenInfo.new(1.2, Enum.EasingStyle.Sine), {
                Position = UDim2.new(0.5, -35, 0, 44)
            }):Play()
            task.wait(1.2)
        end
    end)

    task.spawn(function()
        while isRunning do
            TweenService:Create(stroke, TweenInfo.new(1.4, Enum.EasingStyle.Sine), {
                Thickness = 4,
                Transparency = 0.1
            }):Play()
            task.wait(1.4)

            TweenService:Create(stroke, TweenInfo.new(1.4, Enum.EasingStyle.Sine), {
                Thickness = 2,
                Transparency = 0.25
            }):Play()
            task.wait(1.4)
        end
    end)

    task.wait(5)

    isRunning = false

    local sweep = Instance.new("Frame")
    sweep.Size = UDim2.new(0, 90, 1, 0)
    sweep.Position = UDim2.new(-0.3, 0, 0, 0)
    sweep.BackgroundColor3 = Color3.fromRGB(0, 255, 255)
    sweep.BackgroundTransparency = 0.75
    sweep.Parent = frame

    local swc = Instance.new("UICorner")
    swc.CornerRadius = UDim.new(0, 15)
    swc.Parent = sweep

    TweenService:Create(sweep, TweenInfo.new(0.4), {
        Position = UDim2.new(1.2, 0, 0, 0),
        BackgroundTransparency = 1
    }):Play()

    TweenService:Create(customImage, TweenInfo.new(0.4), {
        ImageTransparency = 1,
        Size = UDim2.new(0, 0, 0, 0)
    }):Play()

    TweenService:Create(frame, fadeOut, {
        Position = UDim2.new(0.5, -200, 0.5, -160),
        BackgroundTransparency = 1
    }):Play()

    TweenService:Create(title, fadeOut, { TextTransparency = 1 }):Play()
    TweenService:Create(subtitle, fadeOut, { TextTransparency = 1 }):Play()
    TweenService:Create(loader, fadeOut, { ImageTransparency = 1 }):Play()
    TweenService:Create(stroke, fadeOut, { Transparency = 1 }):Play()

    task.wait(0.4)
    gui:Destroy()

end)

if not success then
    warn("Intro UI failed:", result)
end

task.wait(0.5)
EcholiteHubTheCondoDestroyerMainFrameGui.Visible = true -- Set main GUI visible after the intro finishes
