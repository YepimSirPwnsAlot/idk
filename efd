local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local UserSettings = game:GetService("UserSettings")
local localPlayer = Players.LocalPlayer

repeat wait() until game:IsLoaded()

local function printLog(level, source, message)
    local timestamp = os.date("[%Y-%m-%d %H:%M:%S]")
    spawn(function()
        print(string.format("%s [%s] [%s] %s", timestamp, source, level, message))
    end)
end

local robloxVersion = "Unknown"
pcall(function()
    local ver = UserSettings():GetService("UserGameSettings").Version
    if ver and ver ~= "" then
        robloxVersion = ver
    end
end)

local inStudio = RunService:IsStudio()
local sessionType = inStudio and "Studio" or "Live Game"
local playerName = localPlayer.Name
local userId = localPlayer.UserId
local jobId = game.JobId
local placeId = game.PlaceId
local gameId = tostring(game.GameId)
printLog("info", "Echolite", "Runtime: " .. sessionType)
printLog("info", "Echolite", "Roblox Client Version: " .. robloxVersion)
printLog("debug", "Echolite", "Client Checker thread: 0x726377d420")
printLog("debug", "Echolite", "Echolite Caching thread: 0x726377d410")
printLog("debug", "Echolite", "WebSockets Cleaner thread: 0x726377d400")
printLog("debug", "Echolite", "Echolite Server thread: 0x726377d3f0")
printLog("info", "Cache", "Reattaching to cached client PID: 15304")
printLog("info", "Cache", "Loaded cache: 1 RBXClients, 2 RBXBytecode, 0 RBXMFlags, 1 RBXRenderViews")
printLog("info", "Echolite", "Creating new RBXClient: 15304")
printLog("info", "RBXClient", "[15304]: Initializing...")
printLog("info", "RBXClient", string.format("[15304]: Version: %s", robloxVersion))
printLog("debug", "Echolite", "Created RBXClient instance @0x283dc5640a0")
printLog("info", "Echolite", "Successfully created RBXClient: 15304")
printLog("debug", "RBXClient", "[15304]: Getting RenderView...")
printLog("info", "RBXClient", "[15304]: RenderView retrieved from Cache: 0x21e64525800")
printLog("debug", "RBXClient", "[15304]: DataModel: 0x21e70c6c238")
printLog("debug", "RBXClient", "[15304]: LocalPlayer: 0x21e72e7c480")
printLog("debug", "RBXClient", string.format("[15304]: LocalPlayer Name: '%s'", playerName))
printLog("debug", "RBXClient", string.format("[15304]: UserId: %s", userId))
printLog("debug", "RBXClient", string.format("[15304]: JobId: %s", jobId ~= "" and jobId or "N/A"))
printLog("debug", "RBXClient", string.format("[15304]: PlaceId: %s", placeId))
printLog("debug", "RBXClient", string.format("[15304]: GameId: %s", gameId))
printLog("debug", "RBXClient", "[15304]: EventConnection: 0x21e83828e00")
printLog("info", "DiscordRPC", "RPC is Enabled")
printLog("debug", "RBXClient", "[15304]: StyledTextLabel: 0x21e84307000")
printLog("info", "RBXClient", "[15304]: StyledTextLabel SSS was set to 0")

-- GUI INSTANCE DECLARATIONS (DESTROYER NAME)
local EcholiteHubTheCondoDestroyerScreenGui = Instance.new("ScreenGui")
local EcholiteHubTheCondoDestroyerMainFrameGui = Instance.new("Frame")
local EcholiteHubTheCondoDestroyerMainFrameUIStroke = Instance.new("UIStroke")
local EcholiteHubTheCondoDestroyerMainFrameUICorner = Instance.new("UICorner")
local EcholiteHubTheCondoDestroyerTopBar = Instance.new("Frame")
local EcholiteHubTheCondoDestroyerTopbarUIStroke = Instance.new("UIStroke")
local EcholiteHubTheCondoDestroyerTopBarUICorner = Instance.new("UICorner") -- Corrected variable usage
local EcholiteHubTheCondoDestroyerMainFrameTextLabelEcholiteName = Instance.new("TextLabel")
local EcholiteHubTheCondoDestroyerSeperator = Instance.new("Frame")
local EcholiteHubTheCondoDestroyerSeperatorUICorner = Instance.new("UICorner")
local EcholiteHubTheCondoDestroyerSeperatorUIStroke = Instance.new("UIStroke")
		
local UI_Stroke_Color = Color3.new(0, 255/255, 255/255) -- Set values between 0 and 1

-- ECHOLITEHUBTHECONDODESTROYERSCREENGUI SETUP
EcholiteHubTheCondoDestroyerScreenGui.Parent = localPlayer:WaitForChild("PlayerGui")
EcholiteHubTheCondoDestroyerScreenGui.ResetOnSpawn = false
EcholiteHubTheCondoDestroyerScreenGui.Name = ("Echolite Hub The Condo Destroyer ScreenGui")
EcholiteHubTheCondoDestroyerScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- ECHOLITEHUBTHECONDODESTROYERMAINFRAMEGUI SETUP
EcholiteHubTheCondoDestroyerMainFrameGui.Name = ("efgrefurgbreugr")
EcholiteHubTheCondoDestroyerMainFrameGui.Parent = EcholiteHubTheCondoDestroyerScreenGui
EcholiteHubTheCondoDestroyerMainFrameGui.Position = UDim2.new(0.5, 0, 0.5, 0) -- Centered
EcholiteHubTheCondoDestroyerMainFrameGui.AnchorPoint = Vector2.new(0.5, 0.5) -- Centered
EcholiteHubTheCondoDestroyerMainFrameGui.Size = UDim2.new(0, 395, 0, 290)
EcholiteHubTheCondoDestroyerMainFrameGui.BorderSizePixel = 0
EcholiteHubTheCondoDestroyerMainFrameGui.BackgroundColor3 = Color3.new(0/255, 0/255, 0/255)
EcholiteHubTheCondoDestroyerMainFrameGui.Visible = false -- Will be set to true after loading

-- MAINFRAME UICORNER AND UI STROKE
EcholiteHubTheCondoDestroyerMainFrameUICorner.Name = ("Echolite Hub The Condo Destroyer Main Frame UI Corner")
