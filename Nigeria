local Players = game:GetService("Players")
local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")
local StarterGui = game:GetService("StarterGui")
local gui = PlayerGui:WaitForChild("UltimateTrollingGuiV6")

local function isAllowed()
	return table.find(allowedPlayers, player.Name) ~= nil
end

if isAllowed() then
	gui.Enabled = true
	StarterGui:SetCore("ChatMakeSystemMessage", {
		Text = "You have permission to use the UTG!";
		Color = Color3.fromRGB(85, 255, 85);
		Font = Enum.Font.SourceSansBold;
		FontSize = Enum.FontSize.Size24;
	})
else
	gui.Enabled = false
end

player.Chatted:Connect(function(message)
	if message:lower() == "/open utg" then
		if isAllowed() then
			gui.Enabled = true
		else
			StarterGui:SetCore("ChatMakeSystemMessage", {
				Text = "You're not on the list!";
				Color = Color3.fromRGB(255, 85, 85);
				Font = Enum.Font.SourceSansBold;
				FontSize = Enum.FontSize.Size24;
			})
		end
	end
end)

local function MakeDraggable()
	local frame = Main
	local dragToggle = false
	local dragStart = nil
	local startPos = nil
	local dragSpeed = 0.25

	local function updateInput(input)
		local delta = input.Position - dragStart
		local newPos = UDim2.new(
			startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y
		)
		TweenService:Create(frame, TweenInfo.new(dragSpeed), {Position = newPos}):Play()
	end

	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if dragToggle and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			updateInput(input)
		end
	end)
end

MakeDraggable()
Main:TweenPosition(UDim2.new(0.382, 0, 0.181, 0), "Out", "Quad", 0.5, true)
