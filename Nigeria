task.spawn(function()
	local colors = {
		Color3.fromRGB(255, 0, 0),
		Color3.fromRGB(255, 255, 0),
		Color3.fromRGB(0, 0, 255),
		Color3.fromRGB(128, 0, 128),
		Color3.fromRGB(255, 165, 0),
		Color3.fromRGB(0, 255, 0),
		Color3.fromRGB(255, 51, 153),
	}

	local currentIndex = 1
	local nextIndex = 2

	while true do
		local currentColor = colors[currentIndex]
		local nextColor = colors[nextIndex]

		for step = 0, 1, 0.02 do
			local lerped = currentColor:Lerp(nextColor, step)
			OpenUtg.TextStrokeColor3 = lerped
			subtext.TextStrokeColor3 = lerped
			RunService.RenderStepped:Wait()
		end

		currentIndex = nextIndex
		nextIndex = (nextIndex % #colors) + 1
	end
end)

-- AntiFling Input
AntiFling.Text = player.Name

AntiFling.FocusLost:Connect(function()
	if AntiFling.Text == "Disable" then
		AntiFling:Destroy()
		return
	end

	for _, p in ipairs(Players:GetPlayers()) do
		if string.find(p.Name:lower(), AntiFling.Text:lower()) then
			AntiFling.Text = p.Name
			break
		end
	end
end)

