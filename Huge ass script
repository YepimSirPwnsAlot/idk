	if ch.Head:FindFirstChild("face") then
		ch.Head.face.Texture = "rbxassetid://121712604747616"
	else
		local newface = Instance.new("Decal", ch.Head)
		newface.Face = Enum.NormalId.Front
		newface.Texture = "rbxassetid://121712604747616"
	end
end)()

