GetRegion(han.CFrame*tcf.p,lBlade,function(Hum,HT) if Hum~=Huma then  Dmg(Hum,Dealt()/3) Sound(as.hit,han,1,mran(90,140)/100) end end)
end
wait(0.04)
end
end;
Break=function(Combonum,anim)
CanCombo=true
