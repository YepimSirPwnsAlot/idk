		mesh.MeshId = "http://www.roblox.com/asset/?id=19106014"
		mesh.TextureId = "rbxassetid://37305301"
	end
	q = Character:GetChildren()
	for i = 1, #q do
		if q[i].ClassName == "Model" or q[i].ClassName == "Part" then
		q[i]:remove()
		end
	end
end)

RightArm:remove()
