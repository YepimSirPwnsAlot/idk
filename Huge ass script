qit["bsp"]="BorderSizePixel"

C3Pro={}

C3Pro['BackgroundColor3']=0

C3Pro['TextColor3']=0

C3Pro['TextStrokeColor3']=0

C3Pro['Color']=0

C3Pro['BorderColor3']=0

C3Pro['BackgroundColor3']=0

C3Pro['BackgroundColor3']=0

 

BNPro={}

BNPro['BrickColor']=0

BNPro['TextColor']=0

BNPro['TextStrokeColor']=0

BNPro['BorderColor']=0

