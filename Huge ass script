Handle2weld.C0=clerp(Handle2weld.C0,cf(-4,0,0)*angles(math.rad(0),math.rad(30),math.rad(0)),.2)
end
		so(162246701,DragonHead,1,5)
		local frontcf = DragonHead.CFrame * CFrame.new(0,0,-5)
