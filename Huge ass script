local Gren = SCRIPTUTIL:WaitForChild'Grenade'
local ShakeFactor = 2
local DamageColor = BrickColor.new'Really red';
local MusicData = {Parent=Torso,ID=4466439348;Pitch=.9;Volume=2;}
local God = false
