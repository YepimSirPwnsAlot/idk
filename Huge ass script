Lastqi={} 

qit = {} 

qit["n"]="Name" 

qit["sc"]="Size" 

qit["cf"]="CFrame"

qit["an"]="Anchored"

qit["can"]="CanCollide"

qit["mat"]="Material"  

qit["bn"]="BrickColor" 

qit["mid"]="MeshId"

qit["tid"]="TextureId"

qit["sca"]="Scale" 

qit["tra"]="Transparency" 

qit["ref"]="Reflectance" 

qit["mtyp"]="MeshType"

qit["P0"]="Part0" 

qit["P1"]="Part1" 

qit["tya"]="TextYAlignment"

qit["txa"]="TextXAlignment"

qit["bc3"]="BackgroundColor3"

qit["tc3"]="TextColor3"

qit["pos"]="Position"

qit["txt"]="Text"

qit["bgt"]="BackgroundTransparency"

qit["txs"]="TextTransparency"

